{
  "headers": [
    {
      "source": "/(.+).(json)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/webm"
        }
      ]
    },
    {
      "source": "/(.+).txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/aac"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "test.amyegan.com/"
        }
      ],
      "destination": "/blog"
    },
    {
      "source": "",
      "has": [
        {
          "type": "host",
          "value": "test.amyegan.com/"
        }
      ],
      "destination": "/blog/first-post"
    }
  ]
}
